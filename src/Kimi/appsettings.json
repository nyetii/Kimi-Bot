{
  "Discord": {
    "Token": ""
  },
  "Kimi": {
    "Prefixes": [
      ".",
      "k!"
    ],
    "LogChannel": 0,
    "Guilds": [
      {
        "Id": 0,
        "Name": "",
        "Modules": {
          "ranking": true,
          "birthday": true,
          "color-roles": true,
          "remind-me": false
        },
        "RankingRoles": {
          "0": 0,
          "1": 0
        },
        "BirthdayRoleId": 0
      }
    ]
  },
  "ConnectionStrings": {
    "Database": "Data Source=Kimi.db"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            },
            {
              "Name": "File",
              "Args": {
                "path": "logs/kimi-.log",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    }
  }
}
